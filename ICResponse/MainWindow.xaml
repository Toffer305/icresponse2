<mui:ModernWindow
        x:Class="ICResponse.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"         
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"         
        Title="ICResponse" IsTitleVisible="True"          
        ContentSource="/Pages/Login.xaml"
        WindowState="Maximized"
        >    
    <mui:ModernWindow.TitleLinks>
        <mui:Link DisplayName="settings" Source="/Pages/Settings.xaml" />
        <mui:Link x:Name="LogOutLink" DisplayName="" Source="/Pages/LogOut.xaml" />
    </mui:ModernWindow.TitleLinks>   

</mui:ModernWindow>
