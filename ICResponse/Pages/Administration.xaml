<UserControl x:Class="ICResponse.Pages.Administration"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI" 
             >
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/AdminContent/OverviewStatus.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="overview" Source="/AdminContent/OverviewStatus.xaml" />
                <mui:Link DisplayName="agent stats" Source="/AdminContent/AgentStats.xaml" />
                <mui:Link DisplayName="call volume" Source="/AdminContent/CallVolume.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>        
    </Grid>
    
</UserControl>
