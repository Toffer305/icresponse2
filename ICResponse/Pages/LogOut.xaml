<UserControl x:Class="ICResponse.Pages.LogOut"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI" 
             >    
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                <TextBlock Text="LOG OUT" Style="{StaticResource Heading2}" Margin="5" HorizontalAlignment="Center"/>
                <TextBlock Text="Are you sure?" Margin="5"/>
                <StackPanel Orientation="Horizontal">
            <mui:ModernButton x:Name="btnLogOut" Content="Log Out" Margin="10" Click="btnLogOut_Click" />
            <mui:ModernButton x:Name="btnCancelLogOut" Content="Cancel" Margin="10" Click="btnCancelLogOut_Click" />
                </StackPanel>
            </StackPanel>
        
</UserControl>
