<Application x:Class="ICResponse.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml"
             Exit="Application_Exit" >
    <Application.Resources>        
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/FirstFloor.ModernUI;component/Assets/ModernUI.xaml"/>
                <ResourceDictionary Source="Assets/ModernUI.ICRTheme.xaml"/>
                <ResourceDictionary Source="Assets/CustomAgentColors.xaml"/>
                <ResourceDictionary Source="Assets/CustomOverviewColors.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        
    </Application.Resources>
</Application>
